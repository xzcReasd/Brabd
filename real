                    BorderSizePixel = 0,
                    Parent = Objects.AlphaOutline,
                    Image = Library.Checkers,
                    ScaleType = Enum.ScaleType.Tile,
                    TileSize = UDim2.new(0, 6, 0, 6),
                    ZIndex = ZIndex,
                })

                Utility.New("UICorner", {
                    Name = "UICorner",
                    Parent = Objects.AlphaImage,
                    CornerRadius = UDim.new(0, 5),
