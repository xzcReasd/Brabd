                }, { BackgroundColor3 = "Inline" })

                Utility.New("UICorner", {
                    Name = "UICorner",
                    Parent = Objects.SaturationPickerOutline,
                    CornerRadius = UDim.new(1, 0),
                })

                ZIndex += 1

